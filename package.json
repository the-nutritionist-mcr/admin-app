    "@aws-cdk/aws-iam": "^1.97.0",
    "@aws-cdk/aws-lambda": "^1.97.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.117.0",
    "@aws-cdk/aws-route53": "^1.97.0",
    "@aws-cdk/aws-route53-targets": "^1.97.0",
    "@aws-cdk/aws-s3": "^1.97.0",
    "@aws-cdk/aws-s3-deployment": "^1.97.0",
