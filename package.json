  },
  "files": [
    "build/**/*",
    "src/schema/schema.graphql",
    "backend",
    "src/infrastructure/**/*",
    "src/cdk-entry.ts",
    "app-config/tsconfig.cdk.json",
    "cdk.json"
  ],
  "scripts": {
    "start": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) node scripts/start.js",
    "cypress:run": "yarn start:prod & wait-on http://localhost:5000 && cypress run",
    "start:prod": "cp backend-outputs.json build && yarn dlx serve -s build",
    "build:backend": "node src/scripts/build-backend.js",
    "build:frontend": "REACT_APP_VERSION_NUMBER=$(git rev-parse --short HEAD) node scripts/build.js",
    "clear-tables": "ts-node src/scripts/do-clear-database.ts",
    "clone-prod-tables-to-dev": "ts-node src/scripts/clone-prod-db.ts",
    "deploy:frontend:dev": "cdk deploy ProductionFrontendStackDev --require-approval never --outputs-file src/frontend-outputs.json",
    "deploy:backend:dev": " cdk deploy DevBackendStackdev --require-approval never --outputs-file backend-outputs.json",
    "deploy:frontend:test": "cdk deploy ProductionFrontendStackTest --require-approval never --outputs-file src/frontend-outputs.json",
    "deploy:backend:test": "cdk deploy DevBackendStacktest --require-approval never --outputs-file backend-outputs.json",
    "deploy:backend:prod": "cdk deploy DevBackendStackprod --require-approval never --outputs-file backend-outputs.json",
    "deploy:backend:ci": "cdk deploy tnm-ci-backend-stack --require-approval never --outputs-file backend-outputs.json",
    "deploy:backend:local": "IS_LOCAL_DEPLOY=true cdk deploy tnm-local-backend-stack --require-approval never --outputs-file backend-outputs.json",
    "deploy:backend:int": "cdk deploy tnm-int-backend-stack --require-approval never --outputs-file backend-outputs.json",
    "destroy:backend:int": "cdk destroy --force tnm-int-backend-stack",
    "destroy:backend:ci": "cdk destroy --force tnm-ci-backend-stack",
    "deploy:frontend:prod": "cdk deploy ProductionFrontendStackProd --require-approval never --outputs-file backend-outputs.json",
    "deploy:all": "yarn deploy:backend && yarn deploy:frontend",
    "lint": "eslint .",
    "tsc:watch": "tsc --project ./app-config/tsconfig.incremental.json --watch",
    "test:noisy": "node scripts/test.js --verbose",
    "test": "node scripts/test.js --silent",
    "start:and-wait": "yarn start & yarn wait-on http://localhost:3000",
    "test:cypress": "cypress run",
    "test:ci": "CI=true node scripts/test.js --silent --coverage",
    "amplify:CustomGraphqlResolver": "tsc && mv ./dist/amplify/backend/function/CustomGraphqlResolver/ts ./amplify/backend/function/CustomGraphqlResolver/src",
    "build:models": "graphql-codegen --config codegen.yml",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose",
      "pre-push": "lint-prepush",
      "commit-msg": "yarn commitlint --edit $1"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "lint-prepush": {
    "base": "main",
    "verbose": true,
    "tasks": {
      "*.{js,ts,jsx,tsx}": [
        "node scripts/test.js --watchAll=false --findRelatedTests"
      ],
      "*.json": [
        "jsonlint"
      ]
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      ".pnp.js",
      "*.config.js",
      "*.config.*.js",
      "*.d.ts",
      "dist",
      "amplify/**",
      "config",
      "scripts",
      "src/API.ts",
      "template",
      "coverage",
      "build",
      "cdk.out"
    ],
    "extends": "benwainwright",
    "parserOptions": {
      "project": [
        "./tsconfig.json",
        "./cypress/tsconfig.json"
      ]
    },
    "rules": {
      "unicorn/no-null": "off",
      "@typescript-eslint/promise-function-async": "off"
    },
    "overrides": [
      {
        "files": [
          "cypress/**/*"
        ],
        "rules": {
          "promise/catch-or-return": "off",
          "promise/always-return": "off"
        }
      },
      {
        "files": [
          "src/infrastructure/**/*"
        ],
        "rules": {
          "no-new": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
